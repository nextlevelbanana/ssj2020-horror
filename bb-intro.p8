pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
cartdata("nlb_batshit_basement")

function _init()
    tick=0
    tx = (128-48)/2
    ty = 128
    done = false
    menu = false
    selected = 1
    cursorx = 64
    cursory = 20
    poke(0x5f2d, 1)
end

function _update()
    if menu then
        cursorx = stat(32)-1
        cursory = stat(33)-1

        if cursory > 42 and cursory < 50 then
            selected = 1
        elseif cursory >= 50 and cursory < 62 then
            selected = 2
        end

        if stat(34) == 1 then
            dset(0, selected)
            load("bb-game1")
        end
    end

    if ty > (128-42)/2 then
        ty -= 1
    else
        tick += 1
    end
    
    if tick > 120 then
        menu = true
    elseif tick > 60 then
        scumm = true
    end
end



function _draw()
    cls()
    if not menu then
        if not scumm then
        spr(129,tx,ty,6,6)
        else
            print("POWERED BY", 44,48,7)
            spr(38,34,60, 8,2)
        end
    else
     --   print(cursorx..cursory, 2,2,10)
        print("USE SWEARS?", 10,40,7)
        print("no thanks", 30, 50, selected == 1 and 11 or 7)
        print((selected == 2 and "fuck" or "f!@#$%").." yeah!", 30, 60, selected == 2 and 11 or 7)
        palt(0,true)
        spr(37, cursorx-3,cursory+3)  
        palt(0,false)

    end
end
