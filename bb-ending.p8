pico-8 cartridge // http://www.pico-8.com
version 29
__lua__

function _init()
cartdata("nlb_batshit_basement")
ending_type = dget(63)
tick = 0
menuitem(1, "clear+start new",startnew)
music(37)
end

function startnew()
	for i=0,63 do
		dset(i,0)
	end
	load("bb-intro")
end

function _update()
 if ending_type == 1 then
  update_bus()
 else
  update_lose()
 end
end

function _draw()
if ending_type == 1 then
  draw_bus()
else
  draw_lose()
 end

end
-->8
function update_bus()
 tick += 1
 if s == nil then
  s = 19
 elseif tick % 5 == 0 then
   if s == 19 then
    s = 23 
   else s = 19
   end
 end
 if tick > 400 and btnp(❎) then
  startnew()
 end
end

function draw_bus()
 cls(15)
 palt(0,true)
 palt(11,true)
 map(0,0,0,20,16,8)
 rectfill(0,84,128,128,0)
 rectfill(0,0,128,20,0)
  palt(0,false)
 spr(s, 30,34,4,4)
 
 if tick < 90 then
  print("FOUR CITY BLOCKS WERE DESTROYED", 2,90,7)
  print("WITHOUT WARNING LAST NIGHT,", 10,100,7)
 elseif tick < 180 then
  print("WHEN A SINKHOLE OPENED UP", 2, 90,7)
  print("UNDER A RESIDENTAL STREET", 10, 100,7)
  print("IN GEORGETOWN.", 18, 110,7)

 elseif tick < 270 then
  print("WITNESSES SAID THEY SAW, QUOTE,", 2,90,7)
 elseif tick < 400 then
    print("'AN UNSPEAKABLE EVIL",2,90,7)
    print("ESCAPING TO THE SKY, CARRYING", 10, 100, 7)
    print("THE SOULS OF THE DAMNED...'", 18, 110,7)
 else
  cls(0)
  palt(11,false)
  print("THE END!", 45,30,11)
  print("you... win?",40,40,11)

  print("(press ❎ to start a new game)", 2,100,7)

 end
 
end

function update_lose()
  tick += 1
 if btnp(❎) then
  startnew()
 end
end

function draw_lose()
  cls(0)
  
  if tick > 150 then
  print("THE END!", 45,30,11)
  print("you lose, loser!",30,60,11)
  print("(press ❎ to start a new game)", 2,100,7)
  else
    step = (1/100)*(tick-30)
    palt(11,false)
    map(0,16,0,24)
    if tick > 30 then
    rectfill(40,24,104,63,0)
    map(5,16,40,24+(step*40),8,5)
    map(0,21,0,64)
    for i=40,104 do
      --if rnd() > 0.8 then
        rectfill(i,64,i,64 - rnd()*16,rnd()*3 +8)
     -- end
    end
    rectfill(0,96,128,128,0)
  end
    
  end

end
